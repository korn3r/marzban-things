{
    "log": {
        "loglevel": "none"
    },
    "inbounds": [
        {
            "tag": "VLESS TCP REALITY",
            "listen": "/opt/run/xray/xray.socket",
            "protocol": "vless",
            "settings": {
                "clients": [
                    {
                        "id": "<ID>",
                        "level": 0,
                        "flow": "xtls-rprx-vision"
                    }
                ],
                "decryption": "none"
            },
            "streamSettings": {
                "network": "tcp",
                "security": "reality",
                "realitySettings": {
                    "dest": "/opt/run/nginx/nginx-443.socket",
                    "serverNames": [ "DOMAIN.COM" ],
                    "alpn": ["h2", "http/1.1"],
                    "minVersion": "1.3",
                    "maxVersion": "1.3",
                    "privateKey": "<PRIVKEY>",
                    "publicKey": "<PUBKEY>",
                    "shortIds": [ "" ],
                    "fingerprint": "chrome",
                    "xver": 1
                },
                "sockopt": {
                    "acceptProxyProtocol": true
                }
            },
            "sniffing": {
                "enabled": false
            }
        }
    ],
    "outbounds": [
        {
            "protocol": "freedom",
            "tag": "DIRECT"
        }
    ]
}
